#LDAPGW part for ldap
#-------------------#
## instance definition LdapClientGW
LdapClientGW_name: "LdapClientGW"
LdapClientGW_ingressqueue_suffix: "ingress"
gw_ram_max: "500M"
gw_ram_min: "300M"
