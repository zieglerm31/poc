      ssl: false
      startTLS: true
      connect_timeout: 1000
      timeout: 1000
      auto_reconnect: true
  - pool2:
    pool_type: 1 # 2 if multiple
    pool_name: dev
    pool_hosts:
      address: tt-b52-vtas3-uircp-ud.tt-b52.cloud.internal
      port: 1389
    pool_options:
      bind_dn: "{{ldap_bind_user}}"
      password: "{{ldap_bind_passwd}}"
      num_connections: 16
      max_connections: 16
      init_connections: 16
      ssl: false
      startTLS: true
      connect_timeout: 1000
      timeout: 1000
      auto_reconnect: true
      
ldap_topology:
  - default:
    site_id: default
    pool_names:
      - sit
      - dev
      
